calc_n_stride <- function(L, w, p) {
stride <- w * (1 - p/100)
ifelse(L <= w, 1L, as.integer(ceiling((L - w) / pmax(stride, .Machine$double.eps)) + 1L))
}
calc_n_extension <- function(L, w, p) {
as.integer(pmax(1, ceiling(L / w - p/100)))
}
